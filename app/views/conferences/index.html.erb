<div class="container text-center">
<h1>Conferences</h1>
  <ul class="list-inline">
    <div class="row g-5">
  <% @conferences.each do |conference| %>
      <div class="col">
        <li class="list-inline-item">
          <%= link_to conference.name, conference %>
        </li>
      </div>
  <% end %>
    </div>
  </ul>
</div>

