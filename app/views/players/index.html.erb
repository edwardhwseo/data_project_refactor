<h1>All Players</h1>

<%= link_to('Back to Home', {controller: 'home', action: 'index'}) %>

<ul>
  <% @players.each do |player| %>
    <li>
      <a href="/players/<%= player.id %>">
        <%= player.first_name %> <%= player.last_name %>
      </a>
    </li>
  <% end %>
</ul>

<%= will_paginate @players %>